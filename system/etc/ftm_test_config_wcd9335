tc 30
#AMIC1 to Headset Codec loopback
!CodecLoop

enable
ADC MUX6:AMIC
AMIC MUX6:ADC2
IIR0 INP0 MUX:DEC6
IIR0 INP0 Volume:75
ADC2 Volume:30
RX INT1 DEM MUX:CLSH_DSM_OUT
RX INT2 DEM MUX:CLSH_DSM_OUT
RX INT1_1 MIX1 INP0:IIR0
RX INT2_1 MIX1 INP0:IIR0
HPHL Volume:80
HPHR Volume:80
RX0 Digital Volume:124
RX1 Digital Volume:124

disable
AMIC MUX6:ZERO
IIR0 INP0 MUX:ZERO
IIR0 INP0 Volume:0
ADC2 Volume:0
RX INT1_1 MIX1 INP0:ZERO
RX INT2_1 MIX1 INP0:ZERO
HPHL Volume:0
HPHR Volume:0
RX0 Digital Volume:0
RX1 Digital Volume:0


