<?xml version="1.0"?>
<category_settings>
    <category>
        <mimetype>vnd.android.cursor.item/phone_v2</mimetype>
        <category_title_ja>電話番号</category_title_ja>
        <category_title_en>Phone number</category_title_en>
        <preset_context_menu_setting>
            <text_ja>発信</text_ja>
            <text_en>Call</text_en>
            <preset_action>KDDI_ADDRESS_CONTEXTMENU_CALL</preset_action>
        </preset_context_menu_setting>
        <custom_context_menu_setting>
            <text_ja>プッシュトーン送信</text_ja>
            <text_en>Send push tone</text_en>
            <package_context>jp.co.sharp.android.addressbook.app</package_context>
            <service_name>jp.co.sharp.android.addressbook.app.AddressBookPlusWrapperService</service_name>
            <action>jp.co.sharp.android.addressbook.action.ADDRESSBOOK_PLUS_SEND_PUSH_TONE</action>
            <key>CALL_NUMBER</key>
        </custom_context_menu_setting>
        <preset_context_menu_setting>
            <text_ja>SMS作成</text_ja>
            <text_en>Create SMS</text_en>
            <preset_action>KDDI_ADDRESS_CONTEXTMENU_SMSTO</preset_action>
        </preset_context_menu_setting>
        <custom_context_menu_setting>
            <text_ja>特番付加発信</text_ja>
            <text_en>Call added special number</text_en>
            <package_context>jp.co.sharp.android.addressbook.app</package_context>
            <service_name>jp.co.sharp.android.addressbook.app.AddressBookPlusWrapperService</service_name>
            <action>jp.co.sharp.android.addressbook.action.ADDRESSBOOK_PLUS_SPECIAL_CALL</action>
            <key>CALL_NUMBER</key>
        </custom_context_menu_setting>
        <preset_context_menu_setting>
            <text_ja>テキストとしてコピー</text_ja>
            <text_en>Copy as text</text_en>
            <preset_action>KDDI_ADDRESS_CONTEXTMENU_COPY</preset_action>
        </preset_context_menu_setting>
        <preset_context_menu_setting>
            <text_ja>メインの電話番号に設定</text_ja>
            <text_en>Set main phone number</text_en>
            <preset_action>KDDI_ADDRESS_CONTEXTMENU_SET_MAIN</preset_action>
        </preset_context_menu_setting>
        <preset_context_menu_setting>
            <text_ja>メインの電話番号を解除</text_ja>
            <text_en>Cancel main phone number</text_en>
            <preset_action>KDDI_ADDRESS_CONTEXTMENU_RELEASE_MAIN</preset_action>
        </preset_context_menu_setting>
    </category>

